{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/?$",
      "dest": "/frontend/userpage.html"
    },
    {
      "src": "/(.*).html",
      "dest": "/frontend/$1.html"
    },
    {
      "handle": "filesystem" 
    },
    {
      "src": "/([^./]+)/?$", 
      "dest": "/frontend/$1.html"
    }
  ]
}
